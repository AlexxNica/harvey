NPROC=1
</$objtype/mkfile

# Note that mksys needs to be in your path, somewhere.
install:V:
	mksys $objtype
	$CC -c *.s
	$AR rv /$objtype/lib/libc.a *.$O
	rm -f *.o *.[$OS] *.s

nuke clean:V:
	rm -f *.[$OS] *.s

installall:V:
	for(objtype in $CPUS) mk install

update:V:
	update $UPDATEFLAGS mkfile sys.h

